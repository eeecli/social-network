{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "linebreak-style": [
            "error",
            "windows"
        ],
        "react/jsx-filename-extension": [
            "warn", {"extensions": [
                    ".js",
                    ".jsx"
                ]
            
            }],
        "react/destructuring-assignment": "off",
        "react/prop-types": "off",
        "no-plusplus": "off",
        "no-debugger": "warn"
    },       
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true
    }
    
}